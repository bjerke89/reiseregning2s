              </Assign.To>
              <Assign.Value>
                <InArgument x:TypeArguments="ui:GenericValue">[item.ProcessName]</InArgument>
              </Assign.Value>
            </Assign>
            <If Condition="[item.ProcessName = &quot;EXCEL&quot;]" sap2010:WorkflowViewState.IdRef="If_4">
