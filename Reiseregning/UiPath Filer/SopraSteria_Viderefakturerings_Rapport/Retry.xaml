            </Assign.Value>
          </Assign>
          <Switch x:TypeArguments="x:Int32" DisplayName="Assign delay time on each retry" Expression="[cd_RetryCounter]" sap2010:WorkflowViewState.IdRef="Switch`1_1">
            <Assign x:Key="2" sap2010:WorkflowViewState.IdRef="Assign_3">
              <Assign.To>
